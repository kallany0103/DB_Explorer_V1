CREATE TABLE rmstudepartment (
    dept_id SERIAL PRIMARY KEY,
    dept_name VARCHAR(100) UNIQUE NOT NULL,
    location VARCHAR(50) DEFAULT 'Main Campus'
);

-- dept_id automatically 1,2,3 হবে
INSERT INTO rmstudepartment (dept_name, location) VALUES
('Computer Science', 'Building A'),
('Electrical Engineering', 'Building B'),
('Business Administration', 'Building C');</sql><sql name="SQL 2*">DROP TABLE IF EXISTS rmstucourse;

CREATE TABLE rmstucourse (
    course_id SERIAL PRIMARY KEY,
    course_name VARCHAR(100) NOT NULL,
    credit INT CHECK (credit &gt; 0 AND credit &lt;= 5),
    dept_id INT NOT NULL,
    CONSTRAINT fk_rmstucourse_rmstudepartment
        FOREIGN KEY (dept_id)
        REFERENCES rmstudepartment(dept_id)
        ON DELETE CASCADE
);

INSERT INTO rmstucourse (course_name, credit, dept_id) VALUES
('Database Systems', 3, 1),
('Computer Networks', 3, 1),
('Circuit Analysis', 4, 2),
('Marketing Basics', 2, 3);
</sql><sql name="SQL 3*">CREATE TABLE rmstustudent (
    student_id SERIAL PRIMARY KEY,            -- Primary Key
    student_name VARCHAR(50) NOT NULL,
    email VARCHAR(100) UNIQUE,                -- Unique constraint
    age INT CHECK (age &gt;= 16 AND age &lt;= 40),  -- Check constraint
    dept_id INT,                              -- Foreign Key
    course_id INT,                            -- Foreign Key

    CONSTRAINT fk_student_department
        FOREIGN KEY (dept_id)
        REFERENCES rmstudepartment(dept_id)
        ON DELETE SET NULL,

    CONSTRAINT fk_student_course
        FOREIGN KEY (course_id)
        REFERENCES rmstucourse(course_id)
        ON DELETE SET NULL
);

